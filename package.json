{
  "name": "firemap.js",
  "version": "1.0.3",
  "description": "The best way to create Heatmaps for any data",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "prepare": "node -e 'require(`prepare-package`)()'",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itw-creative-works/firemap.js"
  },
  "keywords": [
    "firemap.js"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itw-creative-works/firemap.js/issues"
  },
  "homepage": "https://itwcreativeworks.com",
  "dependencies": {
    "canvas": "^2.11.2",
    "itwcw-package-analytics": "^1.0.6"
  },
  "devDependencies": {
    "fs-jetpack": "^4.3.1",
    "mocha": "^8.4.0",
    "prepare-package": "^1.1.5"
  }
}